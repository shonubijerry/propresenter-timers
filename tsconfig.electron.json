{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "outDir": "electron/dist",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "types": ["node", "electron"],
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": ["electron/**/*.ts"]
}
